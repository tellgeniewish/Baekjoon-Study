// tell genie wish 1157
import java.util.Scanner;

public class tgw_BJ1157 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner(System.in);
		String str;
		str = sc.nextLine();
		
		int[] same = new int[str.length()];
		int count;
		for (int i = 0; i < str.length(); i++) {
			count = 0;
			for (int j = 0; j < str.length(); j++) {
				if (str[i].equalsIgnoreCase(str[j]))
					count++;
			}
			same[i] = count;
		}
		
		int max_value = same[0];
		for (int i = 0; i < same.length; i++) {
			if (max_value < same[i])
				max_value = same[i];
		}

		int same_count = 0;
		String[] store = new String[str.length()];
		for (int i = 0; i < str.length(); i++) {
			if (same[i] == max_value) {
				store[i] = str[i];
				same_count++;
			}
		}
		
		int diff_count = 0;
		for (int i = 0; i < same_count; i++) {
			int j = 0;
			while (store[i] != store[j] && j < same_count) {
				j++;
			}
			if (j == same_count)
				diff_count++;
		}
		
		if (diff_count == max_value)
			System.out.println("?");
		else
			System.out.println(str[indexOf(max_value)]);
		
		sc.close();

	}

}
